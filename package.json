{
  "name": "cellows-core",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && gulp build-assets",
    "build:dev": "ng build && gulp build-assets && yarn run libVersion",
    "libVersion": "npm version 0.1.0 --prefix dist/core",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p projects/core/tsconfig.lib.json",
    "composerve": "npx compodoc -s",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "yarn docs:json && start-storybook -s ./projects/core/assets -p 6006",
    "build-storybook": "yarn docs:json && build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.2",
    "@angular/common": "~12.0.2",
    "@angular/compiler": "~12.0.2",
    "@angular/core": "~12.0.2",
    "@angular/forms": "~12.0.2",
    "@angular/platform-browser": "~12.0.2",
    "@angular/platform-browser-dynamic": "~12.0.2",
    "@angular/router": "~12.0.2",
    "@microsoft/applicationinsights-web": "^2.5.11",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.2",
    "@angular/cli": "~12.0.2",
    "@angular/compiler-cli": "~12.0.2",
    "@angular/elements": "^12.0.2",
    "@babel/core": "^7.14.3",
    "@compodoc/compodoc": "^1.1.11",
    "@storybook/addon-actions": "^6.3.0-beta.7",
    "@storybook/addon-essentials": "^6.3.0-beta.7",
    "@storybook/addon-links": "^6.3.0-beta.7",
    "@storybook/angular": "^6.3.0-beta.7",
    "@storybook/builder-webpack5": "^6.3.0-beta.7",
    "@storybook/manager-webpack5": "^6.3.0-beta.7",
    "@storybook/preset-scss": "^1.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@webcomponents/custom-elements": "^1.4.3",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.6",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "gulp-sass-unicode": "^1.0.5",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "sass": "^1.34.1",
    "sass-loader": "^12.0.0",
    "style-loader": "^2.0.0",
    "typescript": "~4.2.4"
  }
}
